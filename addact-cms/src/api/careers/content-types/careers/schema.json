      "target": "api::banner.banner"
    },
    "Careercard": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::global-card.global-card"
    },
    "positions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::position.position"
    },
    "Gallery": {
      "type": "dynamiczone",
      "components": [
        "addact-component.gallery-titles"
      ]
    },
    "PositionsTitle": {
      "type": "component",
      "component": "base-template.title-with-description",
      "repeatable": false
    }
